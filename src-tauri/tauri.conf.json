{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "chattd",
  "version": "0.1.0",
  "identifier": "com.nickisyourfan.chattd",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "chattd",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/512x512.png"
    ]
  }
}
